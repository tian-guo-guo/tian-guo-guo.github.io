<div class="language-text highlighter-rouge"><div class="highlight"><pre class="highlight"><code>function unique(arr){
    var arr2 = arr.sort();
    var res = [arr2[0]];
    for(var i=1;i&lt;arr2.length;i++){
        if(arr2[i] !== res[res.length-1]){
        res.push(arr2[i]);
    }
}
